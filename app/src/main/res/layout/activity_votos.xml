<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VotosActivity">


    <TextView
        android:id="@+id/votos_txt_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Resultados"
        android:textSize="32sp"
        android:layout_centerHorizontal="true"/>


    <TextView
        android:id="@+id/blancos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/votos_txt_title"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        android:text="Blanco:"
        android:textAlignment="textStart"
        android:textSize="32sp" />


    <TextView
        android:id="@+id/nulos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/blancos"
        android:layout_centerHorizontal="true"
        android:text="Nulos: "
        android:textSize="32sp" />


    <TextView
        android:id="@+id/gabriel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/nulos"
        android:layout_centerHorizontal="true"
        android:text="Gabriel Boric: "
        android:textSize="32sp" />


    <TextView
        android:id="@+id/antonio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/gabriel"
        android:layout_centerHorizontal="true"
        android:text="Jose Antonio Kast : "
        android:textSize="32sp" />

    <Button
        android:id="@+id/button"
        android:layout_width="299dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/antonio"

        android:layout_marginStart="40dp"
        android:layout_marginTop="40dp"
        android:onClick="volver"
        android:text="Volver" />


</RelativeLayout>